<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<xliff version="1.0">
    <file target-language="de" datatype="plaintext" original="messages">
        <header/>
        <body>
            <!-- TCA -->
            <trans-unit id="tca.tabs.general">
                <target>Allgemein</target>
            </trans-unit>
            <trans-unit id="tca.tabs.special">
                <target>Speziell</target>
            </trans-unit>
            <trans-unit id="tca.tabs.access">
                <target>Zugriff</target>
            </trans-unit>
            <trans-unit id="tca.palettes.standard">
                <target>Standard</target>
            </trans-unit>
            <trans-unit id="tca.palettes.special">
                <target>Speziell</target>
            </trans-unit>
            <trans-unit id="tca.palettes.visibility">
                <target>Sichtbarkeit</target>
            </trans-unit>

            <!-- Scheduler -->
            <trans-unit id="executeSchedulableCommandTask.name">
                <target>Ausführen von Konsolenbefehlen mit Fortschrittsbalken</target>
            </trans-unit>
            <trans-unit id="executeSchedulableCommandTask.description">
                <target>Ermöglicht die Konfiguration und Ausführung regulärer Konsolenbefehle über das Scheduler-Framework. Ermöglicht die Weitergabe des aktuellen Fortschritts an die Aufgabe.</target>
            </trans-unit>

            <!-- Indexer -->
            <trans-unit id="indexer.page.title">
                <target>Seiten</target>
            </trans-unit>
            <trans-unit id="indexer.tt_content.title">
                <target>Inhaltselemente</target>
            </trans-unit>
            <trans-unit id="indexer.news.title">
                <target>Nachrichten</target>
            </trans-unit>

            <!-- tx_typo3searchalgolia_domain_model_indexingservice -->
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice">
                <target>Indexdienst</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.title">
                <target>Titel</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.title.description">
                <target>Der Titel des Indexdienstes.</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.description">
                <target>Beschreibung</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.description.description">
                <target>Eine optionale Beschreibung für was dieser Indexdienst verwendet wird.</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.search_engine">
                <target>Suchmaschine</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.search_engine.description">
                <target>Wählen Sie die Suchmaschine aus, die zur Indizierung verwendet werden soll.</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.type">
                <target>Typ</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.type.description">
                <target>Wählen Sie den zu verwendenden Indexertyp aus.</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.pages_doktype">
                <target>Seitentyp</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.pages_doktype.description">
                <target>Wählen Sie die Seitentypen aus, nach denen die unten zur Indexierung ausgewählten Seiten gefiltert werden sollen.</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.pages_single">
                <target>Einzelseiten</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.pages_single.description">
                <target>Wählen Sie hier einzelne Seiten aus, die indexiert werden sollen. Die Indexierung erfolgt nur für Seiten, deren Option "In Suche einbeziehen" in den Seiteneigenschaften aktiviert ist.</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.pages_recursive">
                <target>Seiten (rekursiv)</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_indexingservice.pages_recursive.description">
                <target>Wählen Sie hier die Seiten aus, die rekursiv einschließlich aller Unterseiten, indexiert werden sollen. Die Indexierung erfolgt nur für Seiten, deren Option "In Suche einbeziehen" in den Seiteneigenschaften aktiviert ist.</target>
            </trans-unit>

            <!-- tx_typo3searchalgolia_domain_model_searchengine -->
            <trans-unit id="tx_typo3searchalgolia_domain_model_searchengine">
                <target>Suchmaschine</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_searchengine.title">
                <target>Titel</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_searchengine.title.description">
                <target>Der Titel der Suchmaschine.</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_searchengine.description">
                <target>Beschreibung</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_searchengine.description.description">
                <target>Eine optionale Beschreibung für was diese Suchmaschine verwendet wird.</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_searchengine.engine">
                <target>Suchmaschinendienst</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_searchengine.engine.description">
                <target>Wählen Sie den Suchmaschinendienst aus, der für die Suche/Indizierung verwendet werden soll.</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_searchengine.index_name">
                <target>Indexname</target>
            </trans-unit>
            <trans-unit id="tx_typo3searchalgolia_domain_model_searchengine.index_name.description">
                <target>Der Name des Indexes, der für die Speicherung der indizierten Daten innerhalb der Suchmaschine verwendet wird.</target>
            </trans-unit>

            <!-- QueueModule -->
            <trans-unit id="index_queue">
                <target>Warteschlange</target>
            </trans-unit>
            <trans-unit id="index_queue.docheader.button.new">
                <target>Neuer Indexdienst</target>
            </trans-unit>
            <trans-unit id="index_queue.description">
                <target>Die Warteschlange verwaltet die Inhaltsindizierung. Inhalte gelangen durch die unten stehende Initialisierung oder wenn basierend auf der Konfiguration neue Inhalte erstellt werden, in die Warteschlange. Die Elemente in der Warteschlange werden zuerst nach den neuesten Änderungen indexiert, gefolgt von den restlichen Elementen.</target>
            </trans-unit>
            <trans-unit id="index_queue.statistics">
                <target>Warteschlangenstatistik</target>
            </trans-unit>
            <trans-unit id="index_queue.statistics.total">
                <target><![CDATA[Die Warteschlange enthält derzeit <strong>%s</strong> Einträge.]]></target>
            </trans-unit>
            <trans-unit id="index_queue.statistics.last_execution.info">
                <target>Die letzte Indizierung erfolgte am:</target>
            </trans-unit>
            <trans-unit id="index_queue.statistics.last_execution.never">
                <target>noch nie</target>
            </trans-unit>
            <trans-unit id="index_queue.statistics.last_execution.dateFormat">
                <target>d.m.Y H:i:s</target>
            </trans-unit>
            <trans-unit id="index_queue.statistics.column.type">
                <target>Art des Inhalts</target>
            </trans-unit>
            <trans-unit id="index_queue.statistics.column.count">
                <target>Anzahl</target>
            </trans-unit>
            <trans-unit id="index_queue.table.overview">
                <target>Verfügbare Indexdienste</target>
            </trans-unit>
            <trans-unit id="index_queue.table.column.id">
                <target>ID</target>
            </trans-unit>
            <trans-unit id="index_queue.table.column.indexingService">
                <target>Indexdienst</target>
            </trans-unit>
            <trans-unit id="index_queue.indexingServices.available">
                <target>Indexdienste:</target>
            </trans-unit>
            <trans-unit id="index_queue.button.submit">
                <target>Ausgewählten Inhalt zur Indizierung in die Warteschlange stellen</target>
            </trans-unit>
            <trans-unit id="index_queue.information.title">
                <target>Information</target>
            </trans-unit>
            <trans-unit id="index_queue.information.body">
                <target>Es werden jeweils nur die aktiven Indexdienste ausgeführt. Das Initialisieren der Warteschlange ist eine Möglichkeit, eine komplette Neuindizierung zu erreichen oder den Index erstmalig zu befüllen. Die Scheduler-Aufgabe „Index Queue Worker“ indiziert dann die in der Warteschlange aufgelisteten Elemente.</target>
            </trans-unit>
            <trans-unit id="index_queue.flash_message.title">
                <target>Einreihen in die Warteschlange</target>
            </trans-unit>
            <trans-unit id="index_queue.flash_message.body">
                <target>%s Elemente erfolgreich zur Warteschlange hinzugefügt.</target>
            </trans-unit>

            <!-- AdministrationModule -->
            <trans-unit id="administration">
                <target>Administration</target>
            </trans-unit>
            <trans-unit id="administration.description">
                <target>Dieses Modul zeigt eine Übersicht über die von der jeweiligen Suchmaschine verwendeten Indizes und bietet die Möglichkeit, alle Einträge direkt zu löschen.</target>
            </trans-unit>
            <trans-unit id="administration.searchEngine.algolia.headline">
                <target>Algolia</target>
            </trans-unit>
            <trans-unit id="administration.searchEngine.table.column.indexName">
                <target>Indexname</target>
            </trans-unit>
            <trans-unit id="administration.searchEngine.table.column.entries">
                <target>Einträge</target>
            </trans-unit>
            <trans-unit id="administration.searchEngine.table.column.lastUpdate">
                <target>Letzte Aktualisierung</target>
            </trans-unit>
            <trans-unit id="administration.searchEngine.table.column.lastUpdate.dateFormat">
                <target>d.m.Y H:i:s</target>
            </trans-unit>
            <trans-unit id="administration.searchEngine.table.column.task.clear">
                <target>Index leeren</target>
            </trans-unit>

            <!-- Errors -->
            <trans-unit id="error.title">
                <target>Fehler</target>
            </trans-unit>
            <trans-unit id="error.databaseAvailability">
                <target>Bitte stellen Sie sicher, dass die Datenbankstruktur aktuell ist.</target>
            </trans-unit>

            <!-- Flash messages -->
            <trans-unit id="flash_message.error.title">
                <target>Fehler</target>
            </trans-unit>
            <trans-unit id="flash_message.success.title">
                <target>Erfolg</target>
            </trans-unit>
            <trans-unit id="flash_message.success.message.clearIndex">
                <target>Der Index "%s" wurde erfolgreich geleert.</target>
            </trans-unit>
        </body>
    </file>
